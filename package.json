{
  "name": "svelte-5-ui-lib",
  "version": "0.10.10",
  "description": "Svelte 5 UI Lib is a UI library built from scratch to leverage Svelte 5's runes system, creating smooth, reactive components.",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://svelte-5-ui-lib.codewithshin.com/",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/svelte-5-ui-lib"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "gen:exports": "svelte-lib-helpers exports",
    "gen:component-data-from-type": "svelte-lib-helpers component-data-from-type",
    "copy:packagejson": "svelte-lib-helpers package",
    "lib-helpers": "pnpm gen:component-data-from-type && pnpm format && pnpm package && pnpm copy:packagejson",
    "generate-file-list": "tsx scripts/generateFileList.ts",
    "prebuild": "npm run generate-file-list",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@playwright/test": "^1.49.0",
    "@popperjs/core": "^2.11.8",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/enhanced-img": "^0.4.1",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "4.0.1",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.9.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "2.46.0",
    "flowbite-svelte-icons": "^2.0.2",
    "globals": "^15.12.0",
    "highlight.js": "^11.10.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "publint": "^0.2.12",
    "runatics": "^0.1.3",
    "runes-meta-tags": "^0.4.2",
    "svelte": "5.2.4",
    "svelte-check": "^4.0.9",
    "svelte-lib-helpers": "^0.4.16",
    "svelte-rune-highlight": "^0.5.13",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.15",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-imagetools": "^7.0.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "tailwindcss": "^3.0.0"
  },
  "keywords": [
    "svelte 5",
    "sveltekit 2",
    "UI",
    "component library",
    "tailwindcss",
    "flowbite"
  ],
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "apexcharts": "^3.54.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  }
}
